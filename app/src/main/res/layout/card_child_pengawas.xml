    android:orientation="vertical" android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:padding="10dp">
    <LinearLayout
        android:id="@+id/card_childSaksi"
        android:orientation="vertical"
        android:layout_width="fill_parent"
        android:layout_height="wrap_content"
        android:background="@drawable/border_grey">
        <LinearLayout
            android:id="@+id/layoutBarisFoto1"
            android:orientation="horizontal"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp">
            <FrameLayout
                android:id="@+id/frameKandidat1"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:layout_marginBottom="7dp">
                <LinearLayout
                    android:orientation="vertical"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_gravity="center_horizontal"
                    android:layout_marginTop="12dp">
                    <LinearLayout
                        android:orientation="horizontal"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content">
                        <ImageView
                            android:id="@+id/iv_fotoCalon1"
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            android:src="@drawable/user"/>
                        <ImageView
                            android:id="@+id/iv_fotoWakil1"
                            android:layout_width="50dp"
                            android:layout_height="50dp"
                            android:layout_marginLeft="2dp"
                            android:src="@drawable/user"/>
                    </LinearLayout>
                    <TextView
                        android:id="@+id/tv_namaCalon1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Nama Calon 1"
                        android:textSize="12sp"
                        android:gravity="center_horizontal"/>
                    <TextView
                        android:id="@+id/tv_namaWakil1"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:text="Nama Wakil 1"
                        android:textSize="12sp"
                        android:gravity="center_horizontal"/>
                </LinearLayout>
                <TextView
                    android:id="@+id/tv_jmlPemilih1"
                    android:layout_width="30dp"
                    android:layout_height="30dp"
                    android:text="52"
                    android:textSize="20sp"
                    android:textColor="@color/putih"
                    android:background="@drawable/circle_red"
                    android:gravity="center"
                    android:layout_gravity="center_horizontal"/>
            </FrameLayout>
            <FrameLayout
